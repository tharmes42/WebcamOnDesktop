<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <AcrylicBrush
                x:Key="AppChromeAcrylicHostBackdropMediumLowBrush"
                BackgroundSource="HostBackdrop"
                FallbackColor="{ThemeResource SystemChromeMediumColor}"
                TintColor="{ThemeResource SystemChromeLowColor}"
                TintOpacity="0.7" />
    
    <AcrylicBrush x:Key="AppMaxAcrylicBackgroundBrush" BackgroundSource="HostBackdrop"
        TintOpacity="0" TintColor="{ThemeResource SystemChromeLowColor}" FallbackColor="{ThemeResource SystemChromeMediumColor}" />
    

    <Style TargetType="Page" x:Key="PageStyle">
        <!--<Setter Property="Background" Value="{ThemeResource ApplicationPageBackgroundThemeBrush}" />-->
        <Setter Property="Background" Value="{ThemeResource AppMaxAcrylicBackgroundBrush}" />
    </Style>

</ResourceDictionary>
